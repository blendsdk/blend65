module WhileLoopIL;

function main(): void {
    let counter: byte = 0;
    
    // Simple while loop
    while (counter < 10) {
        poke($0400 + counter, counter);
        counter = counter + 1;
    }
    
    // Signal done
    poke($D020, counter);
}