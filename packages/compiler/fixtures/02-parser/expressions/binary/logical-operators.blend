// @fixture: parser/expressions/binary/logical-operators
// @category: parser
// @description: Tests parsing of logical AND/OR operators
// @expect: success
// @output-contains: BEQ
// @tags: expressions, binary, logical, and, or

module LogicalOperators;

function main(): void {
    let a: byte = 5;
    let b: byte = 10;
    let c: byte = 3;
    
    // Logical AND with comparisons
    let andResult: byte = (a < b) && (b > c) ? 1 : 0;
    
    // Logical OR with comparisons
    let orResult: byte = (a > b) || (c < b) ? 1 : 0;
    
    // AND with false condition
    let andFalse: byte = (a > b) && (c < b) ? 1 : 0;
    
    // OR with true condition
    let orTrue: byte = (a < b) || (a > b) ? 1 : 0;
    
    // Chained AND
    let chainAnd: byte = (a < b) && (b > c) && (c > 0) ? 1 : 0;
    
    // Chained OR
    let chainOr: byte = (a > b) || (b < c) || (c < a) ? 1 : 0;
    
    // Mixed AND/OR (AND has higher precedence)
    let mixed: byte = (a < b) || (b > c) && (c > 10) ? 1 : 0;
    
    // With parentheses to override
    let overridden: byte = ((a < b) || (b > c)) && (c < a) ? 1 : 0;
    
    // Multiple comparisons combined
    let complex: byte = (a + 5 > b) && (b - 3 < 10) ? 1 : 0;
    
    // Store results
    poke($0400, andResult);
    poke($0401, orResult);
    poke($0402, mixed);
}