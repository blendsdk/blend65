{
    "name": "@blend65/il",
    "version": "0.1.0",
    "description": "Intermediate Language (IL) system for Blend65 multi-target 6502 compiler",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf dist",
        "dev": "tsc --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@blend65/ast": "*",
        "@blend65/semantic": "*"
    },
    "devDependencies": {
        "typescript": "^5.0.0",
        "vitest": "4.0.16"
    },
    "files": [
        "dist"
    ]
}
