{
  "name": "@blend65/compiler-v2",
  "version": "0.1.0",
  "description": "Blend65 v2 Compiler - Static Frame Allocation architecture for 6502 game development",
  "license": "Elastic-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@types/node": "^25.0.10"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "4.0.18"
  },
  "keywords": [
    "compiler",
    "c64",
    "6502",
    "assembly",
    "blend65",
    "static-frame-allocation"
  ]
}